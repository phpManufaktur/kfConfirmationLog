{% if usage|lower == 'framework' %}
  {% set template_namespace = '@phpManufaktur/Basic/Template' %}
  {% set template_file = 'framework/body.twig' %}
{% elseif usage|lower == 'syncdata' %}
  {% set template_namespace = '@phpManufaktur/ConfirmationLog/Template' %}
  {% set template_file = 'backend/syncdata.body.twig' %}
{% else %}
  {% set template_namespace = '@phpManufaktur/ConfirmationLog/Template' %}
  {% set template_file = 'backend/body.twig' %}
{% endif %}
{% extends getTemplateFile(template_namespace, template_file) %}

{% block stylesheet %}
  {{ parent() }}
  <link rel="stylesheet" type="text/css" href="{{ MANUFAKTUR_URL }}/Basic/Template/default/pattern/css/about.css" media="screen, projection" />
{% endblock stylesheet %}

{% block content %}
  {% include '@phpManufaktur/Basic/Template/default/pattern/about.twig' %}
{% endblock content %}

